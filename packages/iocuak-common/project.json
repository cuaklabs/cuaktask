{
  "root": "packages/iocuak-common",
  "sourceRoot": "packages/iocuak-common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": ["^build"],
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/src/**/*.ts"],
      "outputs": ["{projectRoot}/lib"],
      "options": {
        "commands": [
          "pnpm run build:clean",
          "pnpm run build"
        ],
        "cwd": "packages/iocuak-common"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "inputs": ["{projectRoot}/src/**/*.ts"],
      "outputs": [],
      "options": {
        "lintFilePatterns": ["packages/iocuak-common/src/**/*.ts"]
      }
    },
    "publish": {
      "dependsOn": ["build", "^publish"],
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/**"],
      "outputs": [],
      "options": {
        "commands": [
          "pnpm exec iocuak-publish ./packages/iocuak-common"
        ]
      }
    }
  }
}
