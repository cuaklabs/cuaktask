  "dependencies": {
    "@cuaklabs/iocuak": "workspace:*",
    "@cucumber/cucumber": "8.10.0",
    "chai": "4.3.7",
    "reflect-metadata": "0.1.13",
    "sinon": "15.0.1",
    "sinon-chai": "3.7.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
