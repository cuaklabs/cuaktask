{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "bench:createInstance:js": {
      "cache": false
    },
    "bench:createInstancesByTag:js": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "dist/**",
        "lib/**"
      ]
    },
    "format": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "src/**/*.ts"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "src/**/*.ts"
      ]
    },
    "test": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test:e2e": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test:e2e:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "test:integration:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "test:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "test:js:coverage": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:mutation": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": [
        "reports/mutation/mutation.html"
      ]
    },
    "test:uncommitted": {
      "inputs": [
        "src/**/*.ts"
      ],
      "outputs": []
    },
    "test:unit:js": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "dist/**",
        "lib/**"
      ],
      "outputs": []
    },
    "iocuak-jest-config#format": {
      "inputs": [
        "./lib/**/*.js"
      ],
      "outputs": [
        "./lib/**/*.js"
      ]
    }
  }
}
